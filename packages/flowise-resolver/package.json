{
  "name": "@eleme/flowise-resolver",
  "version": "1.0.0",
  "description": "FlowiseAI GraphQL resolver generator",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts src/example.ts --format esm --dts",
    "dev": "tsup src/index.ts src/example.ts --format esm --dts --watch",
    "test": "jest",
    "start:example": "node dist/example.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
} 