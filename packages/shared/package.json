{
  "name": "@eleme/shared",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "import": "./src/index.ts",
        "types": "./src/index.ts"
      },
      "production": {
        "import": "./dist/index.es.js",
        "require": "./dist/index.cjs.js",
        "types": "./dist/types/index.d.ts"
      },
      "default": {
        "import": "./dist/index.es.js",
        "require": "./dist/index.cjs.js",
        "types": "./dist/types/index.d.ts"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite build --watch",
    "build": "cross-env NODE_ENV=production vite build",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "antd": "^5.0.0",
    "@ant-design/icons": "^5.0.1",
    "events": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.0.0"
  },
  "peerDependencies": {
    "antd": "^5.0.0",
    "@ant-design/icons": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}