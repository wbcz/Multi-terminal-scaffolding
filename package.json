{
  "name": "eleme",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @eleme/web dev",
    "dev:admin": "pnpm --filter @eleme/web dev:admin",
    "dev:merchant": "pnpm --filter @eleme/web dev:merchant",
    "dev:platform": "pnpm --filter @eleme/web dev:platform",
    "dev:all": "pnpm -r dev",
    "build": "pnpm --filter @eleme/web build",
    "build:admin": "pnpm --filter @eleme/web build:admin",
    "build:merchant": "pnpm --filter @eleme/web build:merchant",
    "build:platform": "pnpm --filter @eleme/web build:platform",
    "build:all": "pnpm -r build",
    "serve:admin": "pnpm --filter @eleme/web serve:admin",
    "serve:merchant": "pnpm --filter @eleme/web serve:merchant",
    "serve:platform": "pnpm --filter @eleme/web serve:platform",
    "clean": "pnpm --filter @eleme/web clean",
    "clean:admin": "pnpm --filter @eleme/web clean:admin",
    "clean:merchant": "pnpm --filter @eleme/web clean:merchant",
    "clean:platform": "pnpm --filter @eleme/web clean:platform",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.2.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "antd": "^5.23.1",
    "styled-components": "^6.1.14"
  }
}
