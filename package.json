{
  "name": "eleme",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @wbcz/web dev",
    "dev:admin": "pnpm --filter @wbcz/web dev:admin",
    "dev:merchant": "pnpm --filter @wbcz/web dev:merchant",
    "dev:platform": "pnpm --filter @wbcz/web dev:platform",
    "dev:all": "pnpm -r dev",
    "build": "pnpm --filter @wbcz/web build",
    "build:admin": "pnpm --filter @wbcz/web build:admin",
    "build:merchant": "pnpm --filter @wbcz/web build:merchant",
    "build:platform": "pnpm --filter @wbcz/web build:platform",
    "build:all": "pnpm -r build",
    "serve:admin": "pnpm --filter @wbcz/web serve:admin",
    "serve:merchant": "pnpm --filter @wbcz/web serve:merchant",
    "serve:platform": "pnpm --filter @wbcz/web serve:platform",
    "clean": "pnpm --filter @wbcz/web clean",
    "clean:admin": "pnpm --filter @wbcz/web clean:admin",
    "clean:merchant": "pnpm --filter @wbcz/web clean:merchant",
    "clean:platform": "pnpm --filter @wbcz/web clean:platform",
    "lint": "pnpm -r run lint",
    "type-check": "pnpm -r run type-check",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "commit": "git-cz",
    "preview:admin": "pnpm --filter @wbcz/web preview:admin"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.2.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0",
    "vite": "^5.0.12",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "antd": "^5.23.1",
    "styled-components": "^6.1.14"
  }
}
